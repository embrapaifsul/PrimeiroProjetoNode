<%-include('../cabecalho.ejs')%>
<section class="col-12 col-md-10">
    <div class="card my-4">
        <h5 class="card-header">Edita Estadio</h5>
        <div class="card-body">
            <form method="post">
                <div class="mb-3"></div>
                    <label for="id" class="form-label">ID</label>
                    <input type="text" name="id" readonly id="id" class="form-control" value="<%=Time.id%>">
                </div>
                <div class="mb-3">
                    <label for="nome" class="form-label">Nome</label>
                    <input type="text" name="nome" required id="nome" class="form-control" value="<%=Time.nome%>">
                </div>
                <div class="mb-3">
                    <label for="estadio" class="form-label">Estadio</label>
                    <input type="text" name="estadio" required id="estadio" class="form-control" value="<%=Time.estadio%>">
                </div>
                <div class="mb-3">
                    <input type="submit" value="Salvar" class="form-control">
                </div>
            </form>
        </div>
    </div>
</section>
<%-include('../rodape.ejs')%>